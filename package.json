{
  "name": "moneyforward-collector",
  "version": "3.0.0",
  "description": "Export MoneyForward deposit/withdrawal statements in CSV and take a screenshot.",
  "homepage": "https://github.com/book000/moneyforward-collector",
  "bugs": {
    "url": "https://github.com/book000/moneyforward-collector/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "repository": {
    "url": "git@github.com:book000/moneyforward-collector.git",
    "type": "git"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "fix": "run-z fix:prettier,fix:eslint",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "lint:tsc": "tsc",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix",
    "fix:prettier": "prettier --write src",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.9.31",
    "@book000/node-utils": "1.18.125",
    "@types/node": "22.18.3",
    "eslint": "9.35.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-promise": "7.2.1",
    "prettier": "3.6.2",
    "puppeteer-core": "24.19.0",
    "run-z": "2.1.0",
    "tsx": "4.20.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.16.0+sha512.8066e7b034217b700a9a4dbb3a005061d641ba130a89915213a10b3ca4919c19c037bec8066afdc559b89635fdb806b16ea673f2468fbb28aabfa13c53e3f769",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
