{
  "name": "moneyforward-collector",
  "version": "3.0.0",
  "description": "Export MoneyForward deposit/withdrawal statements in CSV and take a screenshot.",
  "homepage": "https://github.com/book000/moneyforward-collector",
  "bugs": {
    "url": "https://github.com/book000/moneyforward-collector/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "repository": {
    "url": "git@github.com:book000/moneyforward-collector.git",
    "type": "git"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "fix": "run-z fix:prettier,fix:eslint",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "lint:tsc": "tsc",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix",
    "fix:prettier": "prettier --write src",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.9.31",
    "@book000/node-utils": "1.18.125",
    "@types/node": "22.18.9",
    "eslint": "9.37.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-promise": "7.2.1",
    "prettier": "3.6.2",
    "puppeteer-core": "24.20.0",
    "run-z": "2.1.0",
    "tsx": "4.20.6",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.18.1+sha512.77a884a165cbba2d8d1c19e3b4880eee6d2fcabd0d879121e282196b80042351d5eb3ca0935fa599da1dc51265cc68816ad2bddd2a2de5ea9fdf92adbec7cd34",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
