{
  "name": "moneyforward-collector",
  "version": "3.0.0",
  "description": "Export MoneyForward deposit/withdrawal statements in CSV and take a screenshot.",
  "homepage": "https://github.com/book000/moneyforward-collector",
  "bugs": {
    "url": "https://github.com/book000/moneyforward-collector/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "repository": {
    "url": "git@github.com:book000/moneyforward-collector.git",
    "type": "git"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "fix": "run-z fix:prettier,fix:eslint",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "lint:tsc": "tsc",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix",
    "fix:prettier": "prettier --write src",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.9.16",
    "@book000/node-utils": "1.18.44",
    "@types/node": "22.15.33",
    "eslint": "9.30.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.20.0",
    "eslint-plugin-promise": "7.2.1",
    "prettier": "3.6.2",
    "puppeteer-core": "24.11.1",
    "run-z": "2.1.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
